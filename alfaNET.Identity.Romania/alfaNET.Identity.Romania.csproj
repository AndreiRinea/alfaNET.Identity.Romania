<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>alfaNET.Identity.Romania</PackageId>
        <Copyright>Andrei Rinea</Copyright>
        <AssemblyVersion>1.1.0</AssemblyVersion>
    </PropertyGroup>

    <PropertyGroup>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/AndreiRinea/alfaNET.Identity.Romania.git</PackageProjectUrl>
        <RepositoryUrl>https://github.com/AndreiRinea/alfaNET.Identity.Romania.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Authors>Andrei Rinea</Authors>
        <Version>1.1.0</Version>
        <Description>A small library for validating and extracting data from Romanian Personal Numeric Codes (CNPs).</Description>
        <PackageTags>CNP;identity;Romania;PNC</PackageTags>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>

</Project>
